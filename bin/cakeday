#!/bin/bash
if [ "$(id -u)" != "0" ]; then
   echo "This script must be run as root" 1>&2
   exit 1
fi

OUTPUT=$(dumpe2fs $(mount | grep 'on \/ ' | awk '{print $1}') 2>/dev/null | grep 'Filesystem created:')

cat << 'EOF'
     ,,,,,
    _|||||_
   {~*~*~*~}
 __{*~*~*~*}__
`-------------`
EOF
echo "My cake day is: ${OUTPUT:19:${#OUTPUT}}"
